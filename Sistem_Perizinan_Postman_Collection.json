	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Set dynamic variables",
					"pm.globals.set('timestamp', Date.now());",
					"pm.globals.set('today', new Date().toISOString().split('T')[0]);"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Extract session cookie from login response",
					"if (pm.response.headers.get('Set-Cookie')) {",
					"    const cookies = pm.response.headers.get('Set-Cookie');",
					"    const sessionMatch = cookies.match(/laravel_session=([^;]+)/);",
					"    if (sessionMatch) {",
					"        pm.globals.set('session_cookie', `laravel_session=${sessionMatch[1]}`);",
					"    }",
					"}",
					"",
					"// Extract CSRF token from response",
					"const responseText = pm.response.text();",
					"const csrfMatch = responseText.match(/name=\"_token\" value=\"([^\"]+)\"/);",
					"if (csrfMatch) {",
					"    pm.globals.set('csrf_token', csrfMatch[1]);",
					"}",
					"",
					"// Extract IDs from responses for subsequent requests",
					"if (pm.response.json && pm.response.json().id) {",
					"    if (pm.info.requestName.includes('Permohonan')) {",
					"        pm.globals.set('permohonan_id', pm.response.json().id);",
					"    } else if (pm.info.requestName.includes('Penerbitan')) {",
					"        pm.globals.set('penerbitan_id', pm.response.json().id);",
					"    } else if (pm.info.requestName.includes('User')) {",
					"        pm.globals.set('user_id', pm.response.json().id);",
					"    }",
					"}"
				]
			}
		}
